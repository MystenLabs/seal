#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jessiemongeon/seal/docs/node_modules/image-size/bin/node_modules:/Users/jessiemongeon/seal/docs/node_modules/image-size/node_modules:/Users/jessiemongeon/seal/docs/node_modules:/Users/jessiemongeon/seal/node_modules:/Users/jessiemongeon/node_modules:/Users/node_modules:/node_modules:/Users/jessiemongeon/seal/docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jessiemongeon/seal/docs/node_modules/image-size/bin/node_modules:/Users/jessiemongeon/seal/docs/node_modules/image-size/node_modules:/Users/jessiemongeon/seal/docs/node_modules:/Users/jessiemongeon/seal/node_modules:/Users/jessiemongeon/node_modules:/Users/node_modules:/node_modules:/Users/jessiemongeon/seal/docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../image-size/bin/image-size.js" "$@"
else
  exec node  "$basedir/../image-size/bin/image-size.js" "$@"
fi
